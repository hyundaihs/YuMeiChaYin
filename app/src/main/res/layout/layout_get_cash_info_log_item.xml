<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:padding="10dp">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/cash_log_time"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_type"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_money"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_name"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_phone"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_bank_name"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_bank_num"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

        <TextView
            android:id="@+id/cash_log_beizhu"
            style="@style/get_cash_info_item_textstyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现时间:20102366" />

    </LinearLayout>

    <TextView
        android:id="@+id/cash_log_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:padding="10dp"
        android:text="状态:未处理"
        android:textColor="@android:color/white"
        android:textSize="18sp" />
</LinearLayout>